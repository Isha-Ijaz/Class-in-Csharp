using System;

class Car
{
    public string Model;

    public void ShowModel()
    {
        Console.WriteLine(Model);
    }
}

class Program
{
    static void Main()
    {
        Car c1 = new Car();
        Car c2 = new Car();

        c1.Model = "Toyota";
        c2.Model = "Honda";

        c1.ShowModel();
        c2.ShowModel();
    }
}
